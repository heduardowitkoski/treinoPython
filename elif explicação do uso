idade=int(input('Insira sua idade: '))

print('#################')


print('Programa 1(forma correta)')

if idade>=18:
    print('Você é adulto')
elif idade>=15:
    print('Você é adolescente')
else:
    print('Você é criança')

print('#################')


print('Programa 2(forma errada)')

if idade>=18:
    print('Você é adulto')
if idade>=15:
    print('Você é adolescente')
else:
    print('Você é criança')
