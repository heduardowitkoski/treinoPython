def media (*notas):
    soma=0
    for i in range(0,len(notas)):
        soma+=notas[i]
    media=soma/len(notas)
    return media

print('{:.2f}'.format(media(8,9.3,6.7,5,10,7)))

################################################

def cubos (*args):
    cubo=[]
    for i in range(0,len(args)):
        cubo.append((args[i])**3)
    return cubo
print(cubos(1,2,3,4,5))
